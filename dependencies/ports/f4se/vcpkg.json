{
    "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg/master/scripts/vcpkg.schema.json",
    "name": "f4se",
    "version-string": "0.6.23",
    "port-version": 0,
    "description": "Papyrus script sources for the Skyrim Script Extender (F4SE).",
    "homepage": "https://f4se.silverlock.org/",
    "features": {
        "plugin": {
            "description": "Builds the F4SE DLL module.",
            "dependencies": [
                "script-extender-common"
            ]
        },
        "scripts": {
            "description": "Install the F4SE Papyrus script sources."
        },
        "internal-xbyak":{
            "description": "Installs F4SE's internal xbyak library (v4.7.8) (optional)"
        }
    },
    "dependencies": [
        {
            "name": "vcpkg-cmake-config",
            "host": true
        }
    ],
    "default-features": [
        "plugin",
        "scripts"
    ]
}
